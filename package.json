    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "bcrypt": "^1.0.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "connect-busboy": "0.0.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.26.1",
    "escape-html": "^1.0.3",
    "eslint": "^3.9.0",
    "eslint-plugin-react": "^6.4.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.1",
    "file-loader": "^0.9.0",
    "hat": "0.0.3",
