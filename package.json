  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --content-base dist/ --history-api-fallback --hot --port 8765 --open",
    "dist": "rm -rf dist; mkdir dist; NODE_ENV=production ./node_modules/.bin/webpack -p --output-path dist/",
    "start": "node server/run.js",
    "start:dev": "NODE_ENV=development nodemon --ignore ui/ server/run.js",
    "sync": "rsync -r --progress .babelrc LICENSE README.md dist package.json server webapp@www.pbc.org:/srv/webapp/"
  },
