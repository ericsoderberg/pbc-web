    "json-server": "^0.9.2",
    "json-stringify-pretty-compact": "^1.0.2",
    "leaflet": "^1.0.1",
    "markdown": "^0.5.0",
    "markdown-to-jsx": "^4.0.3",
    "moment": "^2.15.2",
    "mongodb": "^2.2.11",
    "mongoose": "^4.6.5",
    "morgan": "^1.7.0",
    "node-sass": "^3.10.1",
    "nodemailer": "^2.6.4",
