{
  "name": "pbc-web",
  "description": "PBC web site",
  "version": "1.0.0",
  "authors": [
    "Eric Soderberg"
  ],
  "bugs": "https://github.com/ericsoderberg/pbc-web/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericsoderberg/pbc-web.git"
  },
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --content-base dist/ --history-api-fallback --hot --open",
    "dist": "rm -rf dist; mkdir dist; cp ui/index.html dist; NODE_ENV=production ./node_modules/.bin/webpack -p --content-base dist/",
    "start": "node server/run.js",
    "start:dev": "NODE_ENV=development nodemon --ignore src/ server/run.js",
    "sync": "rsync -r --progress .babelrc LICENSE README.md dist package.json server webapp@test.pbc.org:/srv/webapp/"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
